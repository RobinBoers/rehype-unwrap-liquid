{
  "name": "remark-unwrap-liquid",
  "version": "4.0.0",
  "description": "Simple remark plugin that removes the enclosing `<p>` and `</p>` from lines that contain only a single liquid tag.",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": "./index.js",
  "files": [
    "lib/",
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "@types/mdast": "^4.0.0",
    "hast-util-to-string": "^3.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "prettier": "^3.0.0",
    "rehype-stringify": "^10.0.0",
    "remark": "^15.0.0",
    "remark-parse": "^11.0.0",
    "remark-preset-wooorm": "^9.0.0",
    "remark-rehype": "^11.0.0",
    "type-coverage": "^2.0.0",
    "typescript": "^5.0.0",
    "unified": "^11.0.0",
    "xo": "^0.56.0"
  },
  "scripts": {
    "format": "prettier . --log-level warn --write && xo --fix"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-wooorm"
    ]
  },
  "xo": {
    "prettier": true
  }
}
